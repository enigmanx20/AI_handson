   "source": [
    "color_map = torch.tensor([\n",
    "    [0, 0, 0],        # 0: non tissue pixel is colorized in black\n",
    "    [255, 255, 255],  # 1: tissue pixel is colorized in white\n",
    "    [0, 255, 0],      # 2: non tumor gland is colorized in green\n",
    "    [0, 0, 255],      # 3: Gleason pattern 3 is colorized in blue\n",
    "    [255, 165, 0],    # 4: Gleason pattern 4 is colorized in orange\n",
    "    [255, 0, 0],      # 5: Gleason pattern 5 is colorized in red\n",
    "], dtype=torch.uint8)  \n",
    "\n",
    "roi_size = 512 # must not exceed width or height\n",
    "update_interval = 200\n",
    "\n",
    "def update():\n",
    "    im = background_screenshot(dcObj, cDC, dataBitMap, width, height, x, y)\n",
    "    im_center = transforms.CenterCrop(roi_size)(im)\n",
    "    \n",
    "    with torch.no_grad():\n",
    "        img_tensor = transforms.Compose([\n",
    "            transforms.Resize((int(roi_size*factor), int(roi_size*factor))),\n",
    "            transforms.ToTensor(),\n",
    "            transforms.Normalize(mean=[0.485, 0.456, 0.406], std=[0.229, 0.224, 0.225])\n",
    "        ])(im_center).unsqueeze(0)\n",
    "        \n",
    "        img_tensor = img_tensor.to(device)\n",
    "        with autocast():\n",
    "            out = model(img_tensor)['out']  # [1, 6, H, W]\n",
    "            out = F.interpolate(out, size=(roi_size, roi_size), mode='bilinear', align_corners=False)\n",
    "            pred = torch.argmax(out, dim=1)[0]  # [H, W], class index map\n",
    "    \n",
    "    # Map class index to color\n",
    "    colorized = color_map[pred]  # [H, W, 3]\n",
    "    colorized = colorized.permute(2, 0, 1)  # [3, H, W]\n",
    "    \n",
    "    # Create a full background and paste the colorized center\n",
    "    background = torch.zeros(3, height, width, dtype=torch.uint8)\n",
    "    h_start = height // 2 - roi_size//2\n",
    "    w_start = width // 2 - roi_size//2\n",
    "    background[:, h_start:h_start+roi_size, w_start:w_start+roi_size] = colorized\n",
    "\n",
    "    out_img = transforms.ToPILImage()(background)\n",
    "    \n",
    "    # Blend with the original\n",
    "    img = Image.blend(im, out_img.convert('RGB'), 0.5)\n",
    "    img = ImageTk.PhotoImage(img)\n",
    "    panel.configure(image=img)\n",
    "    panel.image = img\n",
    "    image_window.after(update_interval, update)\n",
    "\n",
    "# Initialize Tkinter\n",
    "image_window = tk.Tk()\n",
    "im = background_screenshot(dcObj, cDC, dataBitMap, width, height, x, y)\n",
    "img = ImageTk.PhotoImage(im)\n",
    "panel = tk.Label(image_window, image=img)\n",
    "panel.pack(side=\"bottom\", fill=\"both\", expand=\"yes\")\n",
    "\n",
    "factor = mpp / model_mpp\n",
    "\n",
    "image_window.after(update_interval, update)\n",
    "image_window.mainloop()"
   ]
